    "@ks89/angular-modal-gallery": "^7.2.1",
    "@ng-bootstrap/ng-bootstrap": "^3.3.0",
    "@ngx-gallery/core": "^4.0.1",
    "@ngx-gallery/lightbox": "^4.0.1",
    "@ngx-rocket/scripts": "^3.0.1",
    "@ngx-translate/core": "^10.0.1",
    "@tinymce/tinymce-angular": "^2.5.0",
    "@types/file-saver": "^2.0.0",
